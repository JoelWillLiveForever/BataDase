<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Words and sentences -->
    <sys:String x:Key="Text_Login">ログイン</sys:String>
    <sys:String x:Key="Text_Password">パスワード</sys:String>
    <sys:String x:Key="Text_SignIn">サインイン</sys:String>
    <sys:String x:Key="Text_SignUp">サインアップ</sys:String>

    <!-- Tables names -->
    <sys:String x:Key="Text_Carriages">ありがとう</sys:String>
    <sys:String x:Key="Text_Cities">都市</sys:String>
    <sys:String x:Key="Text_Locomotives">ロコモティブ</sys:String>
